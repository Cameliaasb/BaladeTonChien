  <div class="footer">
      <%= link_to tindogs_path, class: "#{"black" if current_page?(tindogs_path)}" do %>
        <img width="32" height="32" src="<%= asset_path('tindog.png') %>" alt="two-dogs"/>
      <% end %>
      <%= link_to matches_path, class: "#{"black" if current_page?(matches_path)}" do %>
        <i class="fa-solid fa-message"></i>
      <% end %>
      <%= link_to dog_walks_path(current_user.dog), class: "#{"black" if current_page?(dog_walks_path(current_user.dog))}" do %>
        <i class="fa-solid fa-map-location-dot"></i>
      <% end %>
      <%= link_to my_dog_path, class: "#{"black" if current_page?(my_dog_path)}" do %>
        <i class="fa-solid fa-dog"></i>
      <% end %>
  </div>
